    "Cl√©ment Borel"
  ],
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@zettlr/citr": "^1.2.2",
    "citeproc": "^2.4.62",
    "js-yaml": "^4.1.0",
    "markdown-it": "^12.3.0",
    "markdown-it-attrs": "^4.1.3",
    "moment": "^2.29.1",
    "nunjucks": "^3.2.3",
    "slugify": "^1.6.5",
    "yaml-front-matter": "^4.1.1"
  },
  "directories": {
    "doc": "docs"
  },
